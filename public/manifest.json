{
  "manifest_version": 3,
  "name": "Neet",
  "description": "Provide Code templates for various algorithms",
  "version": "1.2",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "favicon16.png",
      "32": "favicon32.png"
    }
  },
  "icons": {
    "16": "favicon16.png",
    "32": "favicon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "addon@neet-templates.com"
      }
    },
    "permissions": [
      "storage"
    ]

}